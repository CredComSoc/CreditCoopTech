    "@react-leaflet/core": ">=1.0.0 <1.1.0 || ^1.1.1",
    "@splidejs/splide": "^4.0.7",
    "@splidejs/vue-splide": "^0.6.7",
    "animate.css": "^4.1.1",
    "bootstrap": "^5.1.3",
    "bootstrap-vue": "^2.21.2",
    "bootstrap-vue-3": "^0.1.6",
    "core-js": "^3.6.5",
    "dotenv": "^16.0.3",
    "fetch-no-cors": "^1.0.13",
    "js-cookie": "^2.2.1",
    "jssha": "^3.2.0",
    "react-leaflet": ">=3.1.0 <3.2.0 || ^3.2.1",
    "socket.io-client": "^4.5.1",
    "uuid4": "^2.0.2",
    "vue": "^3.2.31",
    "vue-hotjar-next": "^1.4.0",
    "vue-i18n": "^9.2.2",
    "vue-router": "^4.0.12",
    "vue-window-size": "^1.1.2",
    "vuex": "^4.0.0-0",
